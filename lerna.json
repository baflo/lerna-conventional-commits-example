{
  "lerna": "2.11.0",
  "version": "independent",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "command": {
    "bootstrap": {
      "ci": false,
      "nohoist": [],
      "strict": true
    },
    "exec": {
      "no-bail": false,
      "no-prefix": false,
      "stream": true
    },
    "run": {
      "no-bail": false,
      "no-prefix": false,
      "stream": true
    },
    "publish": {
      "conventionalCommits": true,
      "preid": "webcom",
      "dist-tag": "latest",
      "pre-dist-tag": "internal"
    },
    "version": {
      "allowBranch": "develop",
      "exact": true,
      "conventionalCommits": true,
      "conventionalPrerelease": "*",
      "include-merged-tags": true,
      "message": "chore(prerelease): publish new prerelease versions",
      "noGitTagVersion": false,
      "noPush": true,
      "preid": "webcom",
      "distTag": "latest",
      "preDistTag": "internal"
    }
  }
}
